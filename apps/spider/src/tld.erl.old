-module(tld).
-export([space/1]).
-include("domaininfo.hrl").

space(Space) when is_binary(Space)->
	space(binary_to_list(Space));

space(Space) when is_list(Space)->
	case i_isValid(Space) of
	{ok,Type}->
		#space{space=Space,type=Type,numSegs=numSpaces(Space)};

	Error->
		Error
	end.

numSpaces("au")->
	2;

numSpaces("uk")->
	2;

numSpaces("nz")->
	2;

numSpaces("za")->
	2;

numSpaces(_)->
	1.


